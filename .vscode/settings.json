{
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "files.eol": "\n",
    "rust-analyzer.checkOnSave.command": "clippy",
    "editor.rulers": [
        120
    ],
    "rust-analyzer.diagnostics.disabled": [
        "unresolved-macro-call"
    ],
    "python.formatter": "black"
}
